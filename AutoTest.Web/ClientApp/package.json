  "eslintIgnore": [
    "build/*"
  ],
  "dependencies": {
    "@microsoft/signalr-protocol-msgpack": "3.1.8",
    "@types/enzyme": "3.10.7",
    "@types/react": "file:./src/types/fake-empty",
    "@types/react-redux": "7.1.9",
    "preact": "10.5.2",
    "preact-markup": "2.0.0",
    "preact-render-to-string": "5.1.10",
    "preact-router": "3.2.1",
    "rbx": "2.2.0",
    "react-icons": "3.11.0",
    "react-redux": "7.2.1",
    "react-use-googlelogin": "0.12.5",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.7",
    "ts-date": "3.0.2",
    "tsdef": "0.0.14",
    "uuid-int": "3.0.3"
  },
  "devDependencies": {
    "@teamsupercell/typings-for-css-modules-loader": "2.3.0",
    "@types/jest": "26.0.14",
    "@types/webpack-env": "1.15.3",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "css-loader": "4.3.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-preact-pure": "2.2.3",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.2",
    "eslint-plugin-react-hooks": "4.1.2",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "jest-preset-preact": "4.0.2",
    "lint-staged": "10.4.0",
    "preact-cli": "3.0.1",
    "preact-cli-plugin-env-vars": "1.2.1",
    "prettier": "2.1.2",
    "sirv-cli": "1.0.6",
    "typescript": "4.0.3"
