    "@types/react-redux": "7.1.16",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "eslint-plugin-ts-immutable": "0.3.0",
    "preact": "10.5.13",
    "preact-markup": "2.1.1",
    "preact-render-to-string": "5.1.19",
